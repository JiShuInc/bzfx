<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cardinal.tech.bzfx.dao.TjBaozhangkajlDao">

    <resultMap id="BaseResultMap" type="com.cardinal.tech.bzfx.entity.TjBaozhangkajl">
        <!--@Table tj_baozhangkajl-->
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="did" column="did" jdbcType="VARCHAR"/>
        <result property="ryid" column="ryid" jdbcType="INTEGER"/>
        <result property="dwid" column="dwid" jdbcType="INTEGER"/>
        <result property="zhikayy" column="zhikayy" jdbcType="INTEGER"/>
        <result property="zhikasj" column="zhikasj" jdbcType="TIMESTAMP"/>
        <result property="baozhangkh" column="baozhangkh" jdbcType="VARCHAR"/>
        <result property="creatAt" column="creat_at" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="BaseResultMap">
        select
          id, did, ryid, dwid, zhikayy, zhikasj, baozhangkh, creat_at
        from bzfx.tj_baozhangkajl
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="BaseResultMap">
        select
          id, did, ryid, dwid, zhikayy, zhikasj, baozhangkh, creat_at
        from bzfx.tj_baozhangkajl
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="BaseResultMap">
        select
          id, did, ryid, dwid, zhikayy, zhikasj, baozhangkh, creat_at
        from bzfx.tj_baozhangkajl
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="did != null and did != ''">
                and did = #{did}
            </if>
            <if test="ryid != null">
                and ryid = #{ryid}
            </if>
            <if test="dwid != null">
                and dwid = #{dwid}
            </if>
            <if test="zhikayy != null">
                and zhikayy = #{zhikayy}
            </if>
            <if test="zhikasj != null">
                and zhikasj = #{zhikasj}
            </if>
            <if test="baozhangkh != null and baozhangkh != ''">
                and baozhangkh = #{baozhangkh}
            </if>
            <if test="creatAt != null">
                and creat_at = #{creatAt}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into bzfx.tj_baozhangkajl(did, ryid, dwid, zhikayy, zhikasj, baozhangkh, creat_at)
        values (#{did}, #{ryid}, #{dwid}, #{zhikayy}, #{zhikasj}, #{baozhangkh}, #{creatAt})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update bzfx.tj_baozhangkajl
        <set>
            <if test="did != null and did != ''">
                did = #{did},
            </if>
            <if test="ryid != null">
                ryid = #{ryid},
            </if>
            <if test="dwid != null">
                dwid = #{dwid},
            </if>
            <if test="zhikayy != null">
                zhikayy = #{zhikayy},
            </if>
            <if test="zhikasj != null">
                zhikasj = #{zhikasj},
            </if>
            <if test="baozhangkh != null and baozhangkh != ''">
                baozhangkh = #{baozhangkh},
            </if>
            <if test="creatAt != null">
                creat_at = #{creatAt},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from bzfx.tj_baozhangkajl where id = #{id}
    </delete>

    <!--查询指定行数据-->
    <select id="groupBy" resultType="map">
           select
              ${field},count(1) c
            from bzfx.tj_baozhangkajl
            group by ${field}
    </select>

    <!-- 分页查询 -->
    <select id="queryPageTjBaozhangkajlList" resultMap="BaseResultMap">
            select
                id, did, ryid, dwid, zhikayy, zhikasj, baozhangkh, creat_at            
            from bzfx.tj_baozhangkajl
            <trim prefix="where" prefixOverrides="and|or">
                               <if test="obj.id != null">
                        and id = #{obj.id}
                   </if>
                               <if test="obj.did != null and obj.did != ''">
                        and did = #{obj.did}
                   </if>
                               <if test="obj.ryid != null">
                        and ryid = #{obj.ryid}
                   </if>
                               <if test="obj.dwid != null">
                        and dwid = #{obj.dwid}
                   </if>
                               <if test="obj.zhikayy != null">
                        and zhikayy = #{obj.zhikayy}
                   </if>
                               <if test="obj.zhikasj != null">
                        and zhikasj = #{obj.zhikasj}
                   </if>
                               <if test="obj.baozhangkh != null and obj.baozhangkh != ''">
                        and baozhangkh = #{obj.baozhangkh}
                   </if>
                               <if test="obj.creatAt != null">
                        and creat_at = #{obj.creatAt}
                   </if>
                        </trim>
    </select>

</mapper>