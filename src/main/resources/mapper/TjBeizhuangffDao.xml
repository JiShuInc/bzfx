            </if>
            <if test="shuliang != null">
                and shuliang = #{shuliang}
            </if>
            <if test="shijian != null">
                and shijian = #{shijian}
            </if>
            <if test="creatAt != null">
                and creat_at = #{creatAt}
            </if>
        </where>
    </select>

