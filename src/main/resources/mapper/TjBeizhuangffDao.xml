    <!--通过主键修改数据-->
    <update id="update">
        update bzfx.tj_beizhuangff
        <set>
            <if test="did != null and did != ''">
                did = #{did},
            </if>
            <if test="ryid != null">
                ryid = #{ryid},
            </if>
            <if test="dwid != null">
                dwid = #{dwid},
            </if>
            <if test="xinghao != null and xinghao != ''">
                xinghao = #{xinghao},
            </if>
            <if test="creatAt != null">
                creat_at = #{creatAt},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
