    <select id="queryAll" resultMap="BaseResultMap">
        select
          id, did, ryid, dwid, rymc, dwmc, pinming, xinghao, danwei, shuliang, shijian, creat_at
        from bzfx.tj_beizhuangff
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="did != null and did != ''">
