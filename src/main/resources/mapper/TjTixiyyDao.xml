<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cardinal.tech.bzfx.dao.TjTixiyyDao">

    <resultMap id="BaseResultMap" type="com.cardinal.tech.bzfx.entity.TjTixiyy">
        <!--@Table tj_tixiyy-->
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="did" column="did" jdbcType="VARCHAR"/>
        <result property="dwid" column="dwid" jdbcType="INTEGER"/>
        <result property="yiliaodwdm" column="yiliaodwdm" jdbcType="INTEGER"/>
        <result property="yiliaodwmc" column="yiliaodwmc" jdbcType="VARCHAR"/>
        <result property="creatAt" column="creat_at" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="BaseResultMap">
        select
          id, did, dwid, yiliaodwdm, yiliaodwmc, creat_at
        from bzfx.tj_tixiyy
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="BaseResultMap">
        select
          id, did, dwid, yiliaodwdm, yiliaodwmc, creat_at
        from bzfx.tj_tixiyy
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="BaseResultMap">
        select
          id, did, dwid, yiliaodwdm, yiliaodwmc, creat_at
        from bzfx.tj_tixiyy
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="did != null and did != ''">
                and did = #{did}
            </if>
            <if test="dwid != null">
                and dwid = #{dwid}
            </if>
            <if test="yiliaodwdm != null">
                and yiliaodwdm = #{yiliaodwdm}
            </if>
            <if test="yiliaodwmc != null and yiliaodwmc != ''">
                and yiliaodwmc = #{yiliaodwmc}
            </if>
            <if test="creatAt != null">
                and creat_at = #{creatAt}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into bzfx.tj_tixiyy(did, dwid, yiliaodwdm, yiliaodwmc, creat_at)
        values (#{did}, #{dwid}, #{yiliaodwdm}, #{yiliaodwmc}, #{creatAt})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update bzfx.tj_tixiyy
        <set>
            <if test="did != null and did != ''">
                did = #{did},
            </if>
            <if test="dwid != null">
                dwid = #{dwid},
            </if>
            <if test="yiliaodwdm != null">
                yiliaodwdm = #{yiliaodwdm},
            </if>
            <if test="yiliaodwmc != null and yiliaodwmc != ''">
                yiliaodwmc = #{yiliaodwmc},
            </if>
            <if test="creatAt != null">
                creat_at = #{creatAt},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from bzfx.tj_tixiyy where id = #{id}
    </delete>

    <!--查询指定行数据-->
    <select id="groupBy" resultType="map">
           select
              ${field},count(1) c
            from bzfx.tj_tixiyy
            group by ${field}
    </select>

    <!-- 分页查询 -->
    <select id="queryPageTjTixiyyList" resultMap="BaseResultMap">
            select
                id, did, dwid, yiliaodwdm, yiliaodwmc, creat_at            
            from bzfx.tj_tixiyy
            <trim prefix="where" prefixOverrides="and|or">
                               <if test="obj.id != null">
                        and id = #{obj.id}
                   </if>
                               <if test="obj.did != null and obj.did != ''">
                        and did = #{obj.did}
                   </if>
                               <if test="obj.dwid != null">
                        and dwid = #{obj.dwid}
                   </if>
                               <if test="obj.yiliaodwdm != null">
                        and yiliaodwdm = #{obj.yiliaodwdm}
                   </if>
                               <if test="obj.yiliaodwmc != null and obj.yiliaodwmc != ''">
                        and yiliaodwmc = #{obj.yiliaodwmc}
                   </if>
                               <if test="obj.creatAt != null">
                        and creat_at = #{obj.creatAt}
                   </if>
                        </trim>
    </select>

</mapper>